{
  "owner": "device-mailer-uuid",
  "online": true,
  "meshblu": {
    "version" : "2.0.0",
    "forwarders": {
      "message": {
        "received": [{
          "type": "webhook",
          "url": "${serviceUrl}/events/received",
          "method": "POST",
          "generateAndForwardMeshbluCredentials": true
        }]
      }
    },
    "whitelists": {
      "discover": {
        "view": {
          "*": {}
        }
      }
    }
  }
}
