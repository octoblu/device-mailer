{
  "meshblu": {
    "forwarders": {
      "received": [
        {
          "type": "webhook",
          "url": "https://mailer.octoblu.com/message",
          "method": "POST",
          "generateAndForwardMeshbluCredentials": true
        }
      ]
    }
  },
  "messageSchema": {
    "id": "http://jsonschema.net",
    "type": "object",
    "properties": {
      "from": {
        "id": "http://jsonschema.net/from",
        "type": "string"
      },
      "to": {
        "id": "http://jsonschema.net/to",
        "type": "string"
      },
      "subject": {
        "id": "http://jsonschema.net/subject",
        "type": "string"
      },
      "text": {
        "id": "http://jsonschema.net/text",
        "type": "string"
      }
    },
    "required": [
      "from",
      "to",
      "subject",
      "text"
    ]
  },
  "optionsSchema": {
    "id": "http://jsonschema.net",
    "type": "object",
    "properties": {
      "service": {
        "id": "http://jsonschema.net/service",
        "type": "string"
      },
      "host": {
        "id": "http://jsonschema.net/host",
        "type": "string"
      },
      "port": {
        "id": "http://jsonschema.net/port",
        "type": "integer"
      },
      "secure": {
        "id": "http://jsonschema.net/secure",
        "type": "boolean"
      },
      "auth": {
        "id": "http://jsonschema.net/auth",
        "type": "object",
        "properties": {
          "user": {
            "id": "http://jsonschema.net/auth/user",
            "type": "string"
          },
          "pass": {
            "id": "http://jsonschema.net/auth/pass",
            "type": "string"
          }
        }
      }
    },
    "required": [
      "service",
      "host",
      "port",
      "secure",
      "auth"
    ]
  }
}
